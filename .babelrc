{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "6",
          "browsers": ["last 2 versions"]
        },
        "exclude": ["transform-regenerator"],
        "loose": true,
        "useBuiltIns": true
      }
    ],
    "react"
  ],
  "plugins": ["add-module-exports", "transform-class-properties", "transform-object-rest-spread"]
}
